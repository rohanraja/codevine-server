<div class="page-header">
  <%= link_to source_file_infos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_source_file_info_path(@source_file_info), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show source_file_info</h1>
</div>

<dl class="dl-horizontal">
  <dt>Source code info:</dt>
  <dd><%= @source_file_info.source_code_info_id %></dd>

  <dt>Code:</dt>
  <dd><%= @source_file_info.code %></dd>

  <dt>Relativepath:</dt>
  <dd><%= @source_file_info.relativePath %></dd>

</dl>
