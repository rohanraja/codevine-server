<div class="page-header">
  <%= link_to var_instances_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_var_instance_path(@var_instance), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show var_instance</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @var_instance.name %></dd>

  <dt>Type:</dt>
  <dd><%= @var_instance.type %></dd>

</dl>
